<template>
  <v-app>
    <!-- Top App Bar -->
    <v-app-bar app color="primary" dark>
      <!-- Menu Button to toggle sidebar -->
      <v-btn icon @click="toggleDrawer">
        <v-icon>mdi-menu</v-icon>
      </v-btn>
      <v-toolbar-title>Admin Dashboard</v-toolbar-title>
      <v-spacer></v-spacer>
    </v-app-bar>

    <!-- Sidebar Navigation Drawer -->
    <v-navigation-drawer app v-model="drawer" clipped>
      <v-list dense class="sidebar-list">
        <!-- Sidebar Logo -->
        <v-list-item>
          <v-list-item-avatar>
            <v-img src="https://via.placeholder.com/40" alt="Logo" />
          </v-list-item-avatar>
          <v-list-item-content>
            <v-list-item-title class="sidebar-logo-text">My Admin</v-list-item-title>
          </v-list-item-content>
        </v-list-item>

        <!-- Sidebar Divider -->
        <v-divider></v-divider>

        <!-- Menu Items -->
        <!-- Overview Menu Item -->
        <v-list-item link @click="navigateTo('/')" class="sidebar-item">
          <v-list-item-icon>
            <v-icon>mdi-view-dashboard</v-icon>
          </v-list-item-icon>
          <v-list-item-content>
            <v-list-item-title>Overview</v-list-item-title>
          </v-list-item-content>
        </v-list-item>

        <!-- Event Analysis Menu Item -->
        <v-list-item link @click="navigateTo('/event-analysis')" class="sidebar-item">
          <v-list-item-icon>
            <v-icon>mdi-chart-line</v-icon>
          </v-list-item-icon>
          <v-list-item-content>
            <v-list-item-title>Event Analysis</v-list-item-title>
          </v-list-item-content>
        </v-list-item>
      </v-list>
    </v-navigation-drawer>

    <!-- Main Content Area -->
    <v-main>
      <v-container fluid>
        <router-view />
      </v-container>
    </v-main>

    <!-- Footer -->
    <v-footer app color="primary" dark>
      <span>&copy; 2025 Your Company</span>
    </v-footer>
  </v-app>
</template>

<script>
export default {
  name: 'App',
  data() {
    return {
      drawer: true, // Controls the visibility of the sidebar
    };
  },
  methods: {
    /**
     * Toggles the visibility of the sidebar.
     */
    toggleDrawer() {
      this.drawer = !this.drawer;
    },
    /**
     * Navigates to the specified route.
     * @param {String} route - The route path to navigate to.
     */
    navigateTo(route) {
      this.$router.push(route);
    },
  },
};
</script>

<style>
